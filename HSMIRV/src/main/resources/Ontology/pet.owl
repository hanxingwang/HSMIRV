     -->

    <owl:Restriction>
        <rdfs:subClassOf rdf:resource="#Person"/>
        <owl:onProperty>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://pet.owl/#hasOwner"/>
            </rdf:Description>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Restriction>
    <owl:Class>
        <rdfs:subClassOf rdf:resource="#Cat"/>
        <owl:unionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="#Kitty"/>
